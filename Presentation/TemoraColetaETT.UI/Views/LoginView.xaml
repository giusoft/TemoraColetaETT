<?xml version="1.0" encoding="utf-8" ?><ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             xmlns:vm="clr-namespace:TemoraColetaETT.UI.ViewModels"             x:Class="TemoraColetaETT.UI.Views.LoginView"             x:DataType="vm:LoginViewModel"             Title="Login">    <VerticalStackLayout Padding="30" Spacing="15" VerticalOptions="Center">        <Label Text="Bem-vindo!" FontSize="Header" HorizontalOptions="Center" />        <Entry Placeholder="CPF" Text="{Binding Cpf}" Keyboard="Numeric" />        <Entry Placeholder="Senha" Text="{Binding Senha}" IsPassword="True" />        <Button Text="Entrar" Command="{Binding LoginCommand}" HorizontalOptions="Fill" />        <ActivityIndicator IsRunning="{Binding IsBusy}" IsVisible="{Binding IsBusy}" HorizontalOptions="Center" />    </VerticalStackLayout></ContentPage>